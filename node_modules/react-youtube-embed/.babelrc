{
	"presets": [
		"es2015",
		"stage-3",
		"react"
	],
	"plugins": [
		"syntax-dynamic-import",
		"dynamic-import-node",
		[
			"transform-runtime",
			{
				"polyfill": false,
				"regenerator": true
			}
		],
		"add-module-exports",
		[
			"styled-jsx/babel",
			{
				"plugins": [
					"styled-jsx-plugin-postcss"
				]
			}
		],
		[
			"transform-react-remove-prop-types",
			{
				"mode": "wrap"
			}
		]
	]
}
